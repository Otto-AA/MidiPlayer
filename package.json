{
  "name": "midi-player",
  "version": "1.0.0",
  "description": "Providing a simple interface to read and visualize midi files with javascript. It is based on MIDI.js by mudcube. If you also want to play sounds you should check it out.",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "mocha",
    "build": "webpack",
    "watch-build": "nodemon -e js -x \"npm run build\"",
    "watch-test": "nodemon -e js -x \"npm run delay -- 3000 & npm run test\"",
    "//": "delay -- 3000 is used to start the tests after the build is done. This can vary on different computers",
    "delay": "node ./node_modules/npm-delay"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otto-AA/MidiPlayer.git"
  },
  "author": "A_A",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otto-AA/MidiPlayer/issues"
  },
  "homepage": "https://github.com/otto-AA/MidiPlayer#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "mocha": "^4.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
